# .env.example - 配置文件模板
# 复制此文件为 .env 并根据实际情况修改

# ==================== NATS 配置 ====================

# NATS 服务器地址（必需）
NATS_URLS=nats://username:password@localhost:4222

# ==================== 连接配置 ====================
NATS_CONNECT_TIMEOUT=5
NATS_MAX_RECONNECT_ATTEMPTS=5
NATS_RECONNECT_TIME_WAIT=2

# ==================== TLS/SSL 配置 ====================
NATS_TLS_ENABLED=false
NATS_TLS_INSECURE=false
# NATS_TLS_CA_FILE=/path/to/ca.crt
# NATS_TLS_CERT_FILE=/path/to/client.crt
# NATS_TLS_KEY_FILE=/path/to/client.key
# NATS_TLS_HOSTNAME=nats.example.com

# ==================== 其他配置 ====================
